@page
@model Frontend.Pages.ListModel
@{
}
<h1>Listado Personas</h1>
<table class="table">
    <tr>
            <td>Id</td>
            <td>Nombre</td>
            <td>Edad</td>
            <td>Telefono</td>
            <td>Direccion</td>
    </tr>
     @foreach (var persona in Model.Personas)
    {
        <tr>
            <td>@persona.id</td>
            <td>@persona.nombre</td>
            <td>@persona.edad</td>
            <td>@persona.telefono</td>
            <td>@persona.direccion</td>
        </tr> 
    }

</table>
